{
    "manifest_version" : 2,
    "name" : "Calculator",
    "version" : "1.0",
    "description" : "Get your calculations anywhere",
    "icons" : {
        "1" : "logo-icon.png"
    },
    "browser_action" : {
        "default_icon" : "logo-icon.png",
        "default_popup" : "index.html"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
